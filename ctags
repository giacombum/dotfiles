--langdef=dosini
--langmap=dosini:(fobos)
--regex-dosini=/^\[(.*)\]/\1/s,section/
--regex-dosini=/^(.*)\=(.*)/\1/k,key/

--langdef=markdown
--langmap=markdown:.mkd.md
--regex-markdown=/^#[ \t]+(.*)/\1/h,Heading_L1/
--regex-markdown=/^##[ \t]+(.*)/\1/i,Heading_L2/
--regex-markdown=/^###[ \t]+(.*)/\1/k,Heading_L3/
